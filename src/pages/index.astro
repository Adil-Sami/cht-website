import Layout from "../layouts/_layout.astro"
const handleSubmit = () => {
    const form = document.getElementById('contact-form') as HTMLFormElement; // Cast to HTMLFormElement
    const formData = new FormData(form);
    
    fetch('/api/contact', {
        method: 'POST',
        body: formData,
    })
    .then(response => {
        if (response.ok) {
            alert('Message sent successfully!');
        } else {
            alert('There was an error sending your message.');
        }
    })
    .catch(error => {
        console.error('Error:', error);
        alert('There was an error sending your message.');
    });
};

const response = await fetch("http://cloudhawktech.local/graphql", {
  method: 'POST',
 
  headers: { 'Content-Type': 'application/json' },
  body: JSON.stringify({
    query: `query MyQuery {
  homeSections {
    nodes {
      homepageServices {
        csd {
          fieldGroupName
          heading
          readmore {
            url
          }
          servicepoint {
            li1
            li2
            li3
            li4
            li5
            li6
          }
        }
        dtaservice {
          heading
          readmore {
            url
          }
          servicepoint {
            li1
            li2
            li3
            li4
            li5
          }
        }
        dtService {
          heading
          readmore {
            url
          }
          servicepoint {
            li5
            li4
            li3
            li2
            li1
          }
        }
        itcService {
          heading
          servicepoint {
            li7
            li6
            li4
            li3
            li2
            li1
            li
          }
          readmore {
            url
          }
        }
      }
    }
  }
}
    `,
  }),
});

const json = await response.json();
const nodes = json.data.homeSections.nodes;

// Assuming nodes is an array and each node has homepageServices
const homepageServices = nodes.length > 0 ? nodes[0].homepageServices : null;
const csd = homepageServices ? homepageServices.csd : null;
const dta = homepageServices ? homepageServices.dtaservice : null;
const itc = homepageServices ? homepageServices.itcService : null;
const dt = homepageServices ? homepageServices.dtService : null;
// Changed here
console.log("Response data:", JSON.stringify(json.data));
